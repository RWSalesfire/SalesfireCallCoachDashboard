{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "outputDirectory": ".next",
  "crons": [
    { "path": "/api/cron/analyze-calls", "schedule": "*/15 * * * *" },
    { "path": "/api/cron/aggregate-daily", "schedule": "0 6 * * *" }
  ]
}
